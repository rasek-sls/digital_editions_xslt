    <text><!-- Citattecken » Tankstreck – Apostrof ’ Typografiska citattecken (&#x201d;) ” -->
        <body xml:space="preserve">
            <pb facs="slsa945.12_ms.1-2.tif"/>


              <div type="poem">

                  <lg>
                         <l>Högst</l>
                         <l>över gudar och djävlar</l>
                         <l>skrattar en gammal gud</l>
                         <l>dag och natt</l>
                         <l>outsinbart skallande</l>
                     </lg>
                     <lg>
                         <l>Sin ungdoms dårskap</l>
                         <l>minns den gamle</l>
                         <l>hundratusenåriga skrattsalvor</l>
                         <l>rulla genom evigheten:</l>
                     </lg>
                     <lg>
                         <l>Jag skapade världen</l>
                         <l>och sedan</l>
                         <l>två människor –</l>
                         <l>efter mitt och apors beläte</l>
                         <l>hahahaha!</l>
                     </lg>

              </div>
          
        </body>
